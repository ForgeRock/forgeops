{
  "baseURI" : "http://idm:80/",
  "handler" : {
    "type" : "ReverseProxyHandler",
    "config" : {
      "asyncBehavior" : "non_streaming",
      "connections" : 100,
      "numberOfWorkers" : 20,
      "soTimeout" : "30 seconds"
    }
  },
  "_condition" : "${matches(request.uri.path, '^/static/resource.*')}"
}
